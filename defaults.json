{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true
  },
  "rules": {
    "array-bracket-spacing": 2,
    "arrow-body-style": [2, "always"],
    "arrow-parens": 2,
    "arrow-spacing": 2,
    "brace-style": 2,
    "camelcase": 2,
    "comma-dangle": [2, {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always-multiline",
        "exports": "always-multiline",
        "functions": "always-multiline"
    }],
    "comma-spacing": 2,
    "comma-style": 2,
    "complexity": 2,
    "computed-property-spacing": 2,
    "consistent-return": 2,
    "consistent-this": [2, "self"],
    "curly": 2,
    "dot-location": [2, "property"],
    "dot-notation": 2,
    "eol-last": 2,
    "eqeqeq": 2,
    "func-call-spacing": 2,
    "func-name-matching": 2,
    "func-names": 2,
    "func-style": 2,
    "generator-star-spacing": 2,
    "guard-for-in": 2,
    "indent": [2, 4, {
        "SwitchCase": 1,
        "VariableDeclarator": 0
    }],
    "key-spacing": 2,
    "keyword-spacing": [2, {
        "after": true,
        "before": true
    }],
    "line-comment-position": 2,
    "lines-around-directive": 2,
    "max-depth": 2,
    "max-nested-callbacks": [2, {
        "max": 4
    }],
    "max-params": 2,
    "max-statements-per-line": 2,
    "multiline-ternary": [2, "never"],
    "new-cap": 2,
    "new-parens": 2,
    "newline-after-var": 2,
    "newline-before-return": 2,
    "newline-per-chained-call": 2,
    "no-array-constructor": 2,
    "no-caller": 2,
    "no-confusing-arrow": 2,
    "no-console": 0,
    "no-div-regex": 2,
    "no-duplicate-imports": 2,
    "no-else-return": 2,
    "no-empty-function": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-label": 2,
    "no-floating-decimal": 2,
    "no-global-assign": 2,
    "no-implicit-coercion": 2,
    "no-implied-eval": 2,
    "no-inline-comments": 2,
    "no-invalid-this": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": [2, {
      "allowLoop": true
    }],
    "no-lone-blocks": 2,
    "no-lonely-if": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-multiple-empty-lines": 2,
    "no-nested-ternary": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-octal-escape": 2,
    "no-proto": 2,
    "no-prototype-builtins": 2,
    "no-return-assign": 2,
    "no-return-await": 2,
    "no-self-assign": [2, {
        "props": true
    }],
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": 2,
    "no-tabs": 2,
    "no-template-curly-in-string": 2,
    "no-throw-literal": 2,
    "no-trailing-spaces": 2,
    "no-undef-init": 2,
    "no-undef": 2,
    "no-undefined": 2,
    "no-underscore-dangle": 2,
    "no-unmodified-loop-condition": 2,
    "no-unneeded-ternary": 2,
    "no-unsafe-negation": 2,
    "no-unused-expressions": 2,
    "no-use-before-define": 2,
    "no-useless-call": 2,
    "no-useless-computed-key": 2,
    "no-useless-concat": 2,
    "no-useless-constructor": 2,
    "no-useless-escape": 2,
    "no-useless-rename": 2,
    "no-useless-return": 2,
    "no-var": 2,
    "no-void": 2,
    "no-warning-comments": 2,
    "no-whitespace-before-property": 2,
    "no-with": 2,
    "object-curly-newline": [2, {
      "minProperties": 1
    }],
    "object-property-newline": 2,
    "one-var": [2, "never"],
    "operator-linebreak": 2,
    "padded-blocks": [2, "never"],
    "prefer-arrow-callback": [2, {
      "allowNamedFunctions": true
    }],
    "prefer-const": 2,
    "prefer-reflect": 2,
    "prefer-rest-params": 2,
    "prefer-spread": 2,
    "prefer-template": 2,
    "quotes": [2, "single", "avoid-escape"],
    "radix": [2, "as-needed"],
    "semi": 2,
    "semi-spacing": 2,
    "sort-keys": [2, "asc", {
        "caseSensitive": false,
        "natural": true
    }],
    "space-before-blocks": 2,
    "space-before-function-paren": [2, {
        "anonymous": "always",
        "asyncArrow": "always",
        "named": "always"
    }],
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "space-unary-ops": [2, {
        "nonwords": false,
        "words": true
    }],
    "spaced-comment": [2, "always", {
        "block": {
            "balanced": true,
            "exceptions": ["*"],
            "markers": ["!"]
        }
    }],
    "template-curly-spacing": [2, "always"],
    "unicode-bom": 2,
    "wrap-iife": 2,
    "yield-star-spacing": 2,
    "yoda": 2
  }
}
